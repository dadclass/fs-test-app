    "@types/node": "^10.12.18",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
